---
import BaseHead from '../components/BaseHead.astro';
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro';


interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={"Read.me"} description={"This is a github readme generator."} />
	</head>
	<body>
		<Navbar />
		<slot />
		<Footer />
	</body>
</html>

